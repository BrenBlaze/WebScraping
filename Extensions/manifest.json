{
"name" : "Chrome Browser Extension",
"description": "Extension for Highlighting Words on a page",
"version": "1.0",
"permissions": [
    "activeTab"
],
"browser_action":{
  "default_popup": ["modal.html"]

},

"content_scripts": [{
  "matches": [ "<all_urls>" ],
  "default_popup": ["modal.html"]
  
}],
"manifest_version": 2
}
